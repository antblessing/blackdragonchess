<h1><%= @instructor.first_name %> <%= @instructor.last_name %> </h1>
<%= link_to "Back", instructors_path, class: "btn btn-danger" %>
<% if current_user.admin? %>
<%= link_to "Edit", edit_instructor_path(@instructor), class: "btn btn-primary" %>
<%= link_to "Delete", @instructor, method: :delete, class: "btn btn-danger" %>
<% end %>
